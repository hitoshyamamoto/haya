{
  "name": "hayai",
  "version": "0.1.0",
  "type": "module",
  "description": "Instantly create and manage local SQL, NoSQL, and Vector databases with Docker using a fast and extensible CLI tool for backend developers.",
  "main": "dist/index.js",
  "bin": {
    "hayai": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli/index.ts",
    "start": "node dist/cli/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "database",
    "docker",
    "cli",
    "sql",
    "nosql",
    "vector",
    "backend",
    "development",
    "mariadb",
    "postgresql",
    "redis",
    "qdrant",
    "meilisearch",
    "sqlite",
    "duckdb"
  ],
  "author": "hitoshyamamoto <andrehitoshi.01@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hitoshyamamoto/hayai.git"
  },
  "bugs": {
    "url": "https://github.com/hitoshyamamoto/hayai/issues"
  },
  "homepage": "https://github.com/hitoshyamamoto/hayai#readme",
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^12.1.0",
    "dockerode": "^4.0.7",
    "fs-extra": "^11.3.0",
    "inquirer": "^9.2.12",
    "ora": "^8.2.0",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.8",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.6",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
